{
  "name": "CE-Producer",
  "productName": "CE-Producer",
  "description": "CE-Producer desktop app",
  "version": "0.0.0",
  "private": true,
  "author": "Chris Norman <christno@cisco.com>",
  "copyright": "Â© 2021 Cisco Systems inc.",
  "homepage": "http://example.com",
  "main": "app/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/voipnorm/CE-Producer.git"
  },
  "build": {
    "appId": "com.example.ce-producer",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": {
      "provider": "github"
    },
    "mac": {
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "extendInfo": {
        "NSMicrophoneUsageDescription": "This app requires microphone access."
      }
    }
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "preunit": "webpack --config=build/webpack.unit.config.js --env=test",
    "unit": "electron-mocha temp/specs.js --renderer --color --require source-map-support/register",
    "pree2e": "webpack --config=build/webpack.app.config.js --env=test && webpack --config=build/webpack.e2e.config.js --env=test",
    "e2e": "mocha temp/e2e.js --require source-map-support/register",
    "test": "npm run unit && npm run e2e",
    "start": "node build/start.js",
    "build": "npm test && webpack --config=build/webpack.app.config.js --env=production && electron-builder --publish never",
    "release": "npm test && webpack --config=build/webpack.app.config.js --env=production && electron-builder --publish always"
  },
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "axios": "^0.24.0",
    "axios-retry": "^3.2.4",
    "babel-preset-env": "^1.7.0",
    "chart.js": "^3.6.0",
    "cron": "^1.8.2",
    "electron-log": "^4.4.1",
    "electron-updater": "^4.3.9",
    "fs-jetpack": "^4.1.0",
    "jsxapi": "^5.1.1",
    "keytar": "^7.7.0",
    "qs": "^6.10.1",
    "tabulator-tables": "^5.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.1.0",
    "css-loader": "^5.2.0",
    "electron": "^16.0.2",
    "electron-builder": "^22.5.1",
    "electron-mocha": "^10.0.0",
    "mocha": "^8.3.2",
    "source-map-support": "^0.5.6",
    "spectron": "^15.0.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^3.0.0"
  }
}
